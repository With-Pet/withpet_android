<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.withpet.withpet_android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WithPet">
        <activity
            android:name=".ui.activities.ReviewListActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activities.OthersPetActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activities.MyPetActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activities.OthersProfileActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activities.MyProfileActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activities.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>